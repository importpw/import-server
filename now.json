{
  "version": 2,
  "env": {
    "GITHUB_TOKEN": "@importpw-github-access-token"
  },
  "routes": [
    { "src": "/_next/(.*)", "dest": "/_next/$1" },
    { "src": "^/([^/]*)$", "dest": "/index?repo=$1" },
    { "src": "^/([^/]*)/([^/]*)$", "dest": "/index?org=$1&repo=$2" },
    { "src": "^/([^/]*)/([^/]*)(/(.*))?$", "dest": "/index?org=$1&repo=$2&file=$4" }
  ]
}
